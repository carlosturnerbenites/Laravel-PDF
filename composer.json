{
  "name": "tilume/pdf",
  "description": "PDF exports in Laravel using dompdf",
  "license": "MIT",
  "keywords": [
    "laravel",
    "php",
    "pdf",
    "dompdf",
    "export"
  ],
  "authors": [
    {
      "name": "Carlos Alonso Turner Benites",
      "email": "carlosturnerbenites@gmail.com"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "laravel/framework": "^8.40",
    "dompdf/dompdf": "^0.8.2"
  },
  "require-dev": {},
  "autoload": {
    "psr-4": {
      "Tilume\\PDF\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tilume\\PDF\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Tilume\\PDF\\PDFServiceProvider"
      ],
      "aliases": {
        "PDF": "Tilume\\PDF\\Facades\\PDF"
      }
    }
  }
}
